			</fields>
		</layer>
		<layer name="锌"> <!-- Overwriting layer settings for a feature layer -->
			<fields>
				<field name="HM_NAME" alias="采样点" />
				<field name="ZN" alias="锌含量" />
			</fields>
		</layer>
		<layer name="铜"> <!-- Overwriting layer settings for a feature layer -->
			<fields>
				<field name="HM_NAME" alias="采样点" />
				<field name="CU" alias="铜含量" />
			</fields>
		</layer>
		<layer name="铅"> <!-- Overwriting layer settings for a feature layer -->
			<fields>
